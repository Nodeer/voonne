{var $title = 'Sign in'}

{block content}

<div class="box">
	<form action="">
		<h1>Sign in</h1>

		<div class="form-group">
			<label>Email</label>
			<input type="text" class="form-control">
		</div>
		<div class="form-group">
			<label>Password</label>
			<input type="password" class="form-control">
		</div>

		<input type="submit" class="btn btn-primary">
	</form>

	<a href="" class="btn-bottom">Lost your password?</a>
</div>
